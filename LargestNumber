<!DOCTYPE html>
<html>
<head>
  <title>Find Largest Number</title>
</head>
<body>

  <h2>Largest Number Finder</h2>
  <div id="largestNumber"></div> 

  <script>
    let numbers = [];

    for (let i = 0; i < 5; i++) {
      let input = prompt("Enter number " + (i + 1) + ":");
      let num = parseInt(input);
      numbers.push(num);
    }

    let largest = numbers[0];

    for (let i = 1; i < numbers.length; i++) {
      if (numbers[i] > largest) {
        largest = numbers[i];
      }
    }

    document.getElementById("largestNumber").innerText = "The largest number is: " + largest;
  </script>

</body>
</html>
